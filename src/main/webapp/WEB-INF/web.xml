<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>restaurant-jwduth</display-name>

	<display-name>Java Web Under the Hood</display-name>
	<description>Restaurant Menu</description>

	<servlet>
		<servlet-name>MenuServlet</servlet-name>
		<servlet-class>me.fevvelasquez.jwuth.restaurant.servlets.MenuServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MenuServlet</servlet-name>
		<url-pattern>/menu</url-pattern>
	</servlet-mapping>

</web-app>